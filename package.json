{
  "name": "ssr",
  "version": "1.0.0",
  "author": "Sli-7x",
  "license": "ISC",
  "description": "",
  "main": "index.js",
  "repository": "https://github.com/Sli-7x/ssr-react-typescript",
  "scripts": {
    "dev": "export NODE_ENV=dev&& npm run clean && nodemon ./src/server/serverDev.ts",
    "prebuild": "rimraf dist && mkdir dist",
    "build:client": "export NODE_ENV=production&& export BABEL_ENV=production&& npm run prebuild && webpack --config webpack.config.js",
    "build:server": "export NODE_ENV=server&& npm run tslint && rimraf server.min.js && webpack --config webpack.config.js",
    "start": "export NODE_ENV=production&& nodemon ./src/server/server.ts",
    "clean": "rimraf dist && mkdir dist",
    "tslint": "tslint -c ./tslint.json -p ./tsconfig.json -t stylish",
    "test": "export NODE_ENV=test&& jest --watchAll --no-cache",
    "test:coverage": "export NODE_ENV=test&& jest --coverage"
  },
  "dependencies": {
    "@types/cors": "^2.8.4",
    "axios": "^0.18.0",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "lru-cache": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "4.3.1",
    "react-test-renderer": "^16.4.2",
    "redux": "4.0.0",
    "redux-actions": "^2.6.1",
    "redux-saga": "^0.16.0",
    "redux-saga-test-plan": "^3.7.0",
    "redux-saga-testing": "^1.0.5",
    "serialize-javascript": "^1.5.0",
    "styled-components": "^3.4.5"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^4.0.3",
    "@types/enzyme": "^3.1.13",
    "@types/express": "^4.16.0",
    "@types/helmet": "0.0.39",
    "@types/jest": "^23.3.1",
    "@types/lru-cache": "^4.1.1",
    "@types/node": "^10.9.2",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/react-helmet": "^5.0.7",
    "@types/react-loadable": "^5.4.1",
    "@types/react-redux": "^6.0.6",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-test-renderer": "^16.0.2",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^2.3.0",
    "@types/serialize-javascript": "^1.3.2",
    "@types/webpack": "^4.4.11",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-hot-middleware": "^2.16.4",
    "@types/webpack-node-externals": "^1.6.3",
    "awesome-typescript-loader": "^5.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "copy-webpack-plugin": "^4.5.2",
    "file-loader": "^2.0.0",
    "jest": "^23.5.0",
    "jest-styled-components": "^6.1.1",
    "nodemon": "^1.18.3",
    "raf": "^3.4.0",
    "style-loader": "^0.22.1",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.10.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/tests/*.spec.(ts|tsx|js)"
    ],
    "setupFiles": [
      "<rootDir>config/jest/bootstrap.js"
    ],
    "collectCoverageFrom": [
      "src/client/**/*.tsx",
      "src/client/**/*.ts",
      "!src/client/index.tsx",
      "!src/client/core/**/*.ts",
      "!src/client/routes.tsx",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "collectCoverage": true,
    "bail": true,
    "moduleNameMapper": {
      "^.+\\.s?css$": "identity-obj-proxy"
    },
    "coverageDirectory": "./coverage/"
  }
}
